    function ajouter_vente(produit, stock) {

        var quant = $("#text" + produit);
        // var peremp = new Date($("#peremp" + produit).val());
        if (quant.val() != "") {
            if (quant.val() > 0 && quant.val() < Number(stock) + 1) {
                $("#sub" + produit).html("<i class='fa-solid fa-spinner fa-spin-pulse fa-lg'></i>");
                $.ajax({
                    type: "POST",
                    url: "{{ path('commande_produit_add') }}",
                    data: {produit: produit, quantite: quant.val()},
                    success: function (data) {
                        if (data['id'] == 'ok') {
                            var tot = Number(data['prix']) * Number(data['quantite']);
                            $('#commander').append(" <tr id='tr" + produit + "'>\n" +
                                "                        <td>" + data['designation'] + "</td>\n" +
                                "                        <td>" + data['quantite'] + "</td>\n" +
                                "                        <td class='text-center'>" + data['prix'] + "</td>\n" +
                                "                        <td class='text-center'>" + tot + "</td>\n" +
                                "                        <td> <button class='btn btn-danger mr-2 mb-2'\n" +
                                "                                   title=\"{{ 'Supprimer le produit'|trans }}\" onclick='supprim(" + produit + "," + stock + ")'>\n" +
                                "                                    <i class='fa fa-trash'></i>\n" +
                                "                                </button></td></tr>");

                            $("#total").html(data['total']);
                            $("#valeurtotal").val(data['total']);
                            $("#payer").html(data['total']);
                            $("#netapayer").val(data['total']);
                            $("#td" + produit).html(data['quantite']);

                            $("#annuler" + produit).trigger('click');
                            quant.val("");
                            $("#sub" + produit).html("{{ 'Ajouter'|trans }}");
                            $("#btn" + produit).addClass('disabled');
                            $('#valider').removeClass('disabled');
                        } else {
                            $("#annuler" + produit).trigger('click');
                            quant.val("");
                            $("#sub" + produit).html("{{ 'Ajouter'|trans }}");
                            $("#btn" + produit).addClass('disabled');
                            alert(data['id'])
                        }
                    },
                    error: function () {
                        alert("{{ 'La requête n’a pas abouti'|trans }}");
                    }
                });
            } else {
                alert("{{ 'Veillez verifier la quantité'|trans }} ");
            }
        } else {
            $("#sub" + produit).html("{{ 'Ajouter'|trans }}");
            alert("{{ 'Veillez bien renseigner la quantite'|trans }}");
        }
    }

